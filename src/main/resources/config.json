	"jcsCache.saveCheck.interval": 10, // 10s
	"nettyRequestProxyFilter.maxSendBufferSize": 104857600, // 100m
	"debugManager.maxDebugThreads": 10,
	"cacheStats.verifiedResultThreshold": 100,
	"debugManager.debugEnabled": false,
	"requestKeyGenerator.skipHeaders": ["DATE"],
	"trackerClient.enabled" : false
}