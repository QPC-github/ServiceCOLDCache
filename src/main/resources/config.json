	// "includes": [{
	// "uri": "*"
	// }],
	// "exculdes": [],

	// advanced options
	"adminPort": 55321,
	"debugPort": 32877,
	"debugInfo": true,
	"threadPool.acceptorThreads": 15,
	"threadPool.clientToProxyThreads": 50,
	"threadPool.proxyToServerThreads": 50,
	"update": "LRU",
